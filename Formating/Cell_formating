' Inserts a line after a selection

    Range("A1").Select
        Selection.Insert Shift:=xlDown


' Insert text or formulas in cell

    Range("A1").Select
        ActiveCell.FormulaR1C1 = "Text here"
        
        
' Copy and paste comands on macro

    Range("A1").Select
        Selection.Copy
        or
        ActiveSheet.Paste
